 <div class="content">
      <div class="container">
        <div class="page-header">
          <h1>Fill Scheduale Form</h1>
        </div>
        <div class="row">
          <div class="span6 offset3">
            <h4 class="widget-header"><i class="icon-lock"></i> Scheduale Form</h4>
            <div class="widget-body">
              <div class="center-align">
                <form class="form-horizontal form-signin-signup" action="/Admin/Add" method="Post">
                   FlightNo: <input type="text" name="FlightNo" placeholder="FlihgtNo#"><br />
                   Charges: <input type="text" name="Charges" placeholder="Charges"><br />
                   Total Seats: <input type="text" name="TotalSeats" placeholder="TotalSeats"><br />
                    <br /><br />
                   From: <select name="From">
                        <option value="Lahore">Lahore</option>
                        <option value="Karachi">Karachi</option>
                        <option value="Islamabad">Islamabad</option>
                        <option value="Multan">Multan</option>
                        <option value="Peshawer">Peshawer</option>
                        <option value="Dubai">Dubai</option>
                        <option value="Jaddah">Jaddah</option>
                        <option value="Sigapur">Singapur</option>
                        <option value="Estambol">Estambol</option>
                    </select>
                    To:: <select name="To">
                        <option value="Lahore">Lahore</option>
                        <option value="Karachi">Karachi</option>
                        <option value="Islamabad">Islamabad</option>
                        <option value="Multan">Multan</option>
                        <option value="Peshawer">Peshawer</option>
                        <option value="Dubai">Dubai</option>
                        <option value="Jaddah">Jaddah</option>
                        <option value="Sigapur">Singapur</option>
                        <option value="Estambol">Estambol</option>
                    </select><br /><br />
                    Date<input type="text" name="Date" id="datepicker" placeholder="12/12/1999"><br />
                    <script>
                        $('.datepicker').datepicker();
                    </script>
                   
                  <br /><br />
                  <input type="submit" value="Add Flight" class="btn btn-primary btn-large">
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>



